<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.makery.address.view.PersonSignUpController_P">
   <children>
      <GridPane layoutX="16.0" layoutY="10.0" prefHeight="217.0" prefWidth="368.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Pseudo :" />
            <Label text="E-mail :" GridPane.rowIndex="1" />
            <Label text="Mot de passe :" GridPane.rowIndex="2" />
            <Label text="Confirmer mot de passe :" GridPane.rowIndex="3" />
            <TextField fx:id="pseudoField" GridPane.columnIndex="1" />
            <TextField fx:id="emailField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <PasswordField fx:id="mdpField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <PasswordField fx:id="confirmationMdpField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         </children>
      </GridPane>
      <Button layoutX="200.0" layoutY="252.0" mnemonicParsing="false" onAction="#handleRegister" style="-fx-background-color: green;" text="S'inscrire" />
      <Button layoutX="315.0" layoutY="252.0" mnemonicParsing="false" onAction="#handleCancel" style="-fx-background-color: red;" text="Annuler" />
   </children>
</AnchorPane>
